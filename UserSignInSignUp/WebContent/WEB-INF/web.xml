<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>UserSignInSignUp</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  
  
  <servlet>
    <servlet-name>login1</servlet-name>
    <jsp-file>/Admin/AdminLogin2.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>login1</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>register1</servlet-name>
    <jsp-file>/Admin/AdminSignup.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>register1</servlet-name>
    <url-pattern>/register</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>newuser1</servlet-name>
    <jsp-file>/Admin/AdminSignup.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>newuser1</servlet-name>
    <url-pattern>/newuser</url-pattern>
  </servlet-mapping>
  
  
  
  <servlet>
    <servlet-name>sign1</servlet-name>
    <servlet-class>com.tcs.RegisterServlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>sign1</servlet-name>
    <url-pattern>/signup</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>signin1</servlet-name>
    <servlet-class>com.tcs.LoginServlet.CheckDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>signin1</servlet-name>
    <url-pattern>/signin</url-pattern>
  </servlet-mapping>
  
  <servlet>
    <servlet-name>loginpage1</servlet-name>
    <servlet-class>com.tcs.LoginServlet.LoginServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>loginpage1</servlet-name>
    <url-pattern>/loginpage</url-pattern>
  </servlet-mapping>
  
  
  
  
   <servlet>
    <servlet-name>forget1</servlet-name>
    <jsp-file>/Admin/ForgetPassword.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>forget1</servlet-name>
    <url-pattern>/forget</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>send1</servlet-name>
    <servlet-class>com.tcs.ForgotPassword.CheckDetails</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>send1</servlet-name>
    <url-pattern>/send</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>enternew1</servlet-name>
    <jsp-file>/Admin/PasswordEnter.jsp</jsp-file>
  </servlet>
  <servlet-mapping>
    <servlet-name>enternew1</servlet-name>
    <url-pattern>/enternew</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <servlet-name>update1</servlet-name>
    <servlet-class>com.tcs.ForgotPassword.PasswordUpdate</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>update1</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>